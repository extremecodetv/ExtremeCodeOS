<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Extreme Browser" Height="450" Width="1016"
        Name="window">
    <Grid Background="#FF363636">
        <TextBox x:Name="UrlBox" HorizontalAlignment="Left" Margin="120,6,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="823" Height="25" FontSize="14"/>
        <WebBrowser x:Name="Wind" Margin="0,40,0,0"/>
		<Button x:Name="SearchButton" Content="Search" HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top" Background="#FF707070" Height="24" Width="40" FontSize="11" Foreground="White" Click="Search"/>
		<Button x:Name="RefreshButton" Content="Refresh" HorizontalAlignment="Left" Margin="60,6,0,0" VerticalAlignment="Top" Background="#FF707070" Height="24" Width="45" FontSize="11" Foreground="White" Click="Update"/>

    </Grid>
</Window>
